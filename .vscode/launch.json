  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Resume Example",
      "program": "${workspaceFolder}/build/examples/resume",
      "args": [
        "${workspaceFolder}/images/resume_example.png",
        "${workspaceFolder}/weights/",
        "${workspaceFolder}/outputs/",
        "1"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Python Parseq Debug",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/quants/parseq.py",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/quants/quants_env/bin/python3.11"
    }
  ]
}
